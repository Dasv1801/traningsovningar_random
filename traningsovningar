import time
import random

x = input("hur många repitioner vill du göra? tex 6 st")

# Lista med övningar
traningsovningar_Axlar = ["Axelpress", "Axelfram", "Axelbak hantelrodd"]
traningsovningar_Armar = ["bicepscurl", "tricepspress"]

# Slumpa en övning från Traningsovningar_Axlar
slumpad_axelovning = random.choice(traningsovningar_Axlar)

# Slumpa en övning från Traningsovningar_Armar
slumpad_armovning = random.choice(traningsovningar_Armar)

# printa övningar
print("")

time.sleep(1)
print(x + " st " + slumpad_axelovning)

time.sleep(1)
print(x + " st " + slumpad_armovning)
